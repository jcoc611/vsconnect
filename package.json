{
	"name": "vsconnect",
	"version": "0.2.1",
	"main": "./out/extension.js",
	"engines": {
		"vscode": "^1.39.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jcoc611/vsconnect.git"
	},

	"displayName": "VSConnect",
	"publisher": "jcoc611",
	"description": "The ultimate network client for VS Code",
	"preview": true,
	"categories": [ "Other" ],
	"keywords": [ "network", "client", "http", "dns", "utility", "postman" ],
	"qna": "https://github.com/jcoc611/vsconnect/issues",
	"galleryBanner": {
		"color": "#302f33",
		"theme": "dark"
	},

	"activationEvents": [
		"onCommand:vsConnectClient.start",
		"onWebviewPanel:vsConnectClient"
	],
	"contributes": {
		"commands": [
			{
				"command": "vsConnectClient.start",
				"title": "Open a new VSConnect Tab",
				"category": "VSConnect"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./ && webpack",
		"compile-extension": "tsc -p ./",
		"package": "vsce package",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./ && webpack --watch",
		"watch-webpack": "webpack --watch"
	},
	"author": "Juan Osorio <jcoc611@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/classnames": "^2.2.9",
		"@types/node": "^12.11.5",
		"@types/react": "^16.9.9",
		"@types/react-dom": "^16.9.2",
		"@types/request": "^2.48.3",
		"@types/request-promise-native": "^1.0.17",
		"@types/vscode": "^1.39.0",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vsce": "^1.75.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.7"
	}
}
