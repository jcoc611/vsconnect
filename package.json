{
	"name": "vsconnect",
	"version": "0.1.0",
	"description": "The ultimate network client for VS Code",
	"main": "./out/extension.js",
	"engines": {
		"vscode": "^1.38.0"
	},
	"activationEvents": [
		"onCommand:catCoding.start",
		"onCommand:catCoding.doRefactor",
		"onWebviewPanel:catCoding"
	],
	"contributes": {
		"commands": [
			{
				"command": "catCoding.start",
				"title": "Open a new VSConnect Tab",
				"category": "VSConnect"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./ && webpack",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./ && webpack --watch",
		"watch-webpack": "webpack --watch"
	},
	"author": "Juan Osorio <Osorio.Juan@microsoft.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/classnames": "^2.2.9",
		"@types/node": "^12.11.5",
		"@types/react": "^16.9.9",
		"@types/react-dom": "^16.9.2",
		"@types/request": "^2.48.3",
		"@types/request-promise-native": "^1.0.17",
		"@types/vscode": "^1.39.0",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.7"
	}
}
